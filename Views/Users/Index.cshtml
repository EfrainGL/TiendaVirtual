@model IEnumerable<TiendaVirtual.ViewModel.UserView>
@{
    ViewBag.Title = "Index";
}

<h2> Administrador de Permisos</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>

        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
       
            <td>
                @Html.ActionLink("Roles", "Roles", new { UserId = item.userId })
            </td>
        </tr>
    }

</table>
